{
  "name": "@prisma-cms/nextjs",
  "author": "@Fi1osof",
  "license": "MIT",
  "version": "1.6.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "generate:types": "NODE_PATH=. ts-node -P tsconfig.server.json src/modules/gql/cli/generateTypes/run",
    "types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test": "jest",
    "test:api": "TEST_REAL_API=true jest",
    "test:all": "yarn lint && yarn types && yarn cy:run && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run types"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "@storybook/react": "^6.0.28",
    "apollo-upload-client": "^14.1.3",
    "cross-fetch": "^3.0.6",
    "graphql": "^15.3.0",
    "http-proxy-middleware": "^1.0.5",
    "next": "^10.0.5",
    "next-seo": "^4.17.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.0",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/near-operation-file-preset": "^1.17.11",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.7",
    "@next/bundle-analyzer": "^9.5.5",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/css": "^0.0.31",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.1.3",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.161",
    "@types/node": "^13.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "@types/urijs": "^1.19.12",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "cross-env": "^7.0.2",
    "cypress": "^6.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "lodash": "^4.17.20",
    "moment-locales-webpack-plugin": "^1.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3",
    "typescript-styled-plugin": "^0.15.0"
  },
  "description": "Bootstrap a developer-friendly NextJS app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma-cms/nextjs.git"
  },
  "keywords": [
    "prisma-cms",
    "next-js",
    "typescript",
    "ts-node",
    "graphql",
    "headless-cms"
  ],
  "bugs": {
    "url": "https://github.com/prisma-cms/nextjs/issues"
  },
  "homepage": "https://github.com/prisma-cms/nextjs#readme"
}
